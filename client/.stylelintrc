{
  "extends": "stylelint-config-recommended",
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "value"
        ]
      }
    ],
    "no-missing-end-of-source-newline": [true],
    "property-no-unknown": [
      true,
      {
        "ignoreProperties": [
            "composes",
            "compose-with"
        ]
      }
    ],
  }
}